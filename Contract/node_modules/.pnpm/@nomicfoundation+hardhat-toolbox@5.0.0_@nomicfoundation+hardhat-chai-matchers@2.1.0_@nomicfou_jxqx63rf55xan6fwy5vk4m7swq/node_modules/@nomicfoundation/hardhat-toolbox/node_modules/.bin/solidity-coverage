#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/plugins/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/plugins/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/solidity-coverage@0.8.17_hardhat@2.28.0/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../solidity-coverage@0.8.17_hardhat@2.28.0/node_modules/solidity-coverage/plugins/bin.js" "$@"
fi
