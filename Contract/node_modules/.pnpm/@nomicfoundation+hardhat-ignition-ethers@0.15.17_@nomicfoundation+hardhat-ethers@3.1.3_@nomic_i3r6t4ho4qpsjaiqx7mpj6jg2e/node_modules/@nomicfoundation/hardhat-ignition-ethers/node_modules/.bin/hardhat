#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/cli/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/cli/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules:/Users/nongzhang/Documents/web3-study/作业练习/web3-university/Contract/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../../../../../hardhat@2.28.0_ts-node@10.9.2_typescript@5.9.3/node_modules/hardhat/internal/cli/bootstrap.js" "$@"
fi
